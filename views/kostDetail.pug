extends base
block append head
 

  link(href='https://api.mapbox.com/mapbox-gl-js/v1.8.1/mapbox-gl.css' rel='stylesheet')

block content
    main
      section.section-kostDetail-deskripsi
        .img-box
          .container
            .row
              .col-lg-8(data-aos='zoom-in')
                img(src=`/img/kost/images/${kostData.images[0]}`, alt='gambar 1', width='752', height='501')
              .col-lg-4(data-aos='zoom-in')
                .row
                  .col-lg-12.pl-5
                    img(src=`/img/kost/images/${kostData.images[1]}` alt='gambar 2', width='372', height='248')
                  .col-lg-12.pl-5.pt-3
                    img(src=`/img/kost/images/${kostData.images[2]}`, alt='gambar 3', width='372', height='248')
        .container.mt-5
          h1.heading-secondary(data-aos='fade-up')= kostData.name
          .row
            .col-lg-8(data-aos='zoom-in')
              nav
                #nav-tab.nav.nav-tabs(role='tablist')
                  a#nav-home-tab.nav-item.nav-link.active(data-toggle='tab', href='#nav-deskripsi', role='tab', aria-controls='nav-home', aria-selected='true') Deskripsi
                  a#nav-profile-tab.nav-item.nav-link(data-toggle='tab', href='#nav-fasilitas', role='tab', aria-controls='nav-profile', aria-selected='false') Fasilitas kost dan kamar
                  a#nav-contact-tab.nav-item.nav-link(data-toggle='tab', href='#nav-luas', role='tab', aria-controls='nav-contact', aria-selected='false') Luas Kamar
                  a#nav-contact-tab.nav-item.nav-link(data-toggle='tab', href='#nav-lokasi', role='tab', aria-controls='nav-contact', aria-selected='false') Lokasi Kost
                  a#nav-contact-tab.nav-item.nav-link(data-toggle='tab', href='#nav-maps', role='tab', aria-controls='nav-contact', aria-selected='false') Reviews
              #nav-tabContent.tab-content
                #nav-deskripsi.tab-pane.fade.show.active(role='tabpanel', aria-labelledby='nav-home-tab')
                  p.tab-content-paragraph= kostData.deskripsi
                #nav-fasilitas.tab-pane.fade(role='tabpanel', aria-labelledby='nav-profile-tab')
                  .row.ml-3
                    each facilities in kostData.facilities
                      .col-lg-3
                        .row
                          i(class=`${facilities.iconCode} huge-font-size primary-text`)
                          p.medium-font-size.pl-3= facilities.iconName
                  
                #nav-luas.tab-pane.fade(role='tabpanel', aria-labelledby='nav-contact-tab')
                  .row.ml-3
                    i.fas.fa-home.huge-font-size.primary-text
                    p.pl-3.medium-font-size= kostData.roomSize
                #nav-lokasi.tab-pane.fade(role='tabpanel', aria-labelledby='nav-contact-tab')
                  .row
                    #map(style='width: 100% !important; height: 100%;' data-locations=`${JSON.stringify(kostData.location)}`)
                #nav-maps.tab-pane.fade(role='tabpanel', aria-labelledby='nav-contact-tab')
